    "test": "npx tap --no-timeout --test-env=PGUSER=${PGUSER} --test-env=PGHOST=${PGHOST} --test-env=PGPORT=${PGPORT} --test-env=PGDATABASE=${PGDATABASE} --test-env=PGPASSWORD=${PGPASSWORD} --test-env=EOD_TOKEN=${EOD_TOKEN} src/test/*.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
